{
  "name": "backend-typescript-template",
  "version": "1.0.0",
  "description": "A template for backend apis made with typescript",
  "main": "dist/bootstrap.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "preinstall": "clear:modules",
    "clear:modules": "rm -rf node_modules",
    "start:dev": "ts-node-dev --inspect --transpile-only --ignore node_modules src/bootstrap.ts",
    "start:prod": "node dist/bootstrap.js"
  },
  "dependencies": {
    "@types/node": "^16.11.12",
    "dotenv": "^10.0.0",
    "puppeteer": "^13.0.0",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.6.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.4"
  }
}
